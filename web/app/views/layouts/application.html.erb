    <%= render 'layouts/alerts' %>

    <% if @banner_ad && @banner_ad.image.attached? %>
      <%= image_tag @banner_ad.banner %>
    <% end %>




    <%= yield %>

  </body>

  <%= render 'layouts/footer' %>




</html>