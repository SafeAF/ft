<%= search_form_for @q, html: { class: 'form' } do |f| %>

  <div class="form-group">
    <%= f.label :name_cont, class: 'form-label' %>
    <%= f.search_field :name_cont, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.submit "Search", class: "btn btn-primary" %>
  </div>

<% end %>
